{
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.6.2"
    },
    {
      "src": "pago/**/*.php",
      "use": "vercel-php@0.6.2"
    },
    {
      "src": "pse/**/*.php",
      "use": "vercel-php@0.6.2"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/$1"
    },
    {
      "src": "/pago/(.*)",
      "dest": "/pago/$1"
    },
    {
      "src": "/pse/(.*)",
      "dest": "/pse/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
